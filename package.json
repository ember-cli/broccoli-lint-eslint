{
  "name": "broccoli-lint-eslint",
  "version": "6.0.0",
  "description": "broccoli filter that runs eslint",
  "keywords": [
    "broccoli",
    "broccoli-eslint",
    "broccoli-lint",
    "broccoli-plugin",
    "eslint",
    "esprima",
    "jshint",
    "jslint",
    "lint",
    "validate"
  ],
  "homepage": "https://github.com/ember-cli/broccoli-lint-eslint",
  "bugs": {
    "url": "https://github.com/ember-cli/broccoli-lint-eslint/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ember-cli/broccoli-lint-eslint.git"
  },
  "license": "MIT",
  "author": "Jonathan Kingston",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "mocha test/*test.js test/**/*test.js"
  },
  "dependencies": {
    "aot-test-generators": "^0.1.0",
    "broccoli-concat": "^3.7.5",
    "broccoli-persistent-filter": "^2.3.1",
    "eslint": "^7.29.0",
    "json-stable-stringify": "^1.0.1",
    "lodash.defaultsdeep": "^4.6.1",
    "md5-hex": "^3.0.1"
  },
  "devDependencies": {
    "broccoli": "^0.16.9",
    "broccoli-cli": "^1.0.0",
    "broccoli-merge-trees": "^3.0.1",
    "broccoli-source": "^3.0.1",
    "broccoli-stew": "^3.0.0",
    "broccoli-test-helper": "^2.0.0",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.0.0",
    "co": "^4.6.0",
    "mocha": "^6.2.2",
    "p-finally": "^2.0.1",
    "release-it": "^12.4.3",
    "release-it-lerna-changelog": "^1.0.3",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.5.0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  }
}
